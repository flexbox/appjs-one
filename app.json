{
  "expo": {
    "name": "Word Check",
    "slug": "word-check",
    "privacy": "public",
    "owner": "jonsamp",
    "scheme": "wordcheck",
    "platforms": ["ios", "android"],
    "version": "40.1",
    "orientation": "default",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/icon-1.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#151617"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "1",
      "bundleIdentifier": "jonsamp.words",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 2,
      "package": "com.jonsamp.wordcheck",
      "permissions": [],
      "icon": "./assets/images/icon-1.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-foreground.png",
        "backgroundImage": "./assets/images/adaptive-background.png"
      }
    }
  }
}
