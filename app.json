{
  "expo": {
    "name": "Word Check",
    "slug": "word-check",
    "privacy": "public",
    "scheme": "wordcheck",
    "platforms": ["ios", "android"],
    "version": "45.0",
    "orientation": "default",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/icon-1.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#151617"
    },
    "updates": {
